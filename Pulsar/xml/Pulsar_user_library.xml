<!-- $Header$
   This is a library of sources that can be included by a source with an empty element:
   that is, <source name="chime"/> picks up the entry named "chime"
   Also, if the second command line option matches one of these names, it will be used as a source
   (overriding any specified in the xml).
   Finally, it is used to generate a drop-down menu of sources to add.
-->
<!-- ************************************************************************** -->
<!-- Flux and period from 2 EGRET Catalog -->
<!-- Pdot from A.Harding, astro-ph/0208421v1, "Gamma Rays from Rotation powered pulsars -->
<!-- Parameters from H.I.Neil, O.C. De Jager, Astr Space Science 230, 229 -->

<source_library title="GammaPSR_PhenNJ95">
<!-- Polar Cap model. When N,DJ95 give upper or lower limit, use more detailed DeJager 2002 -->
    <source name=" PSRCrabPolarPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params=" 1,2.3e-6,0.033,422e-15,2,1e6,30e6,-1.9,0.29"/>
       <galactic_dir l="184.56" b="-5.78"/>
       </spectrum> 
    </source>
    <source name=" PSRVelaPolarPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params=" 1,9e-6,0.089,124e-15,2,1e6,6e6,-1.7,2.0"/>
       <direction theta="0" phi="0"/>
       <!--galactic_dir l="263.55" b="-2.79"/-->
       </spectrum> 
    </source>
    <source name=" PSRGemingaPolarPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,3.66e-6,0.237,11.4e-15,2,1e6,4.8e6,-1.56,2.2"/>
       <galactic_dir l="195.13" b="4.27"/>
       </spectrum> 
    </source>
    <source name=" PSR1706-44PolarPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,1.23e-6,0.102,92.2e-15,1,1e6,40e6,-1.3,0.5"/>
       <galactic_dir l="343.10" b="-2.69"/>
    </spectrum> 
    </source>
    <source name=" PSR1055-52PolarPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,0.22e-6,0.197,5.8e-15,1,1e6,0.4e6,-0.3,0.8"/>
       <galactic_dir l="285.98" b="6.65"/>
       </spectrum> 
    </source>
<!-- Outer Gap Model, same spectral index, but now cutoff parameter is 1 according to Cheng, ApJ431, 724 (1994) -->
    <source name=" PSRCrabOuterPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params=" 1,2.3e-6,0.033,422e-15,2,1e6,30e6,-1.9,1.0"/>
       <galactic_dir l="184.56" b="-5.78"/>
       </spectrum> 
    </source>
    <source name=" PSRVelaOuterPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params=" 1,9e-6,0.089,124e-15,2,1e6,6e6,-1.7,1.0"/>
       <direction theta="0" phi="0"/>
       <!--galactic_dir l="263.55" b="-2.79"/-->
       </spectrum> 
    </source>
    <source name=" PSRGemingaOuterPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,3.66e-6,0.237,11.4e-15,2,1e6,4.8e6,-1.56,1.0"/>
       <galactic_dir l="195.13" b="4.27"/>
       </spectrum> 
    </source>
    <source name=" PSR1706-44OuterPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,1.23e-6,0.102,92.2e-15,1,1e6,40e6,-1.3,1.0"/>
       <galactic_dir l="343.10" b="-2.69"/>
    </spectrum> 
    </source>
    <source name=" PSR1055-52OuterPhen " >
       <spectrum escale="MeV"> <SpectrumClass name="PulsarSpectrum" params="1,0.22e-6,0.197,5.8e-15,1,1e6,0.4e6,-0.3,1.0"/>
       <galactic_dir l="285.98" b="6.65"/>
       </spectrum> 
    </source>
</source_library>
